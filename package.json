{
  "name": "qnury-es",
  "private": true,
  "version": "0.0.0",
  "description": "Qnurye's personal website and API",
  "scripts": {
    "dev:astro": "pnpm --filter @qnury-es/astro dev",
    "dev:api": "pnpm --filter @qnury-es/worker-api dev",
    "dev:email": "pnpm --filter @qnury-es/worker-api dev:email",
    "build": "pnpm --filter @qnury-es/astro build",
    "preview": "pnpm --filter @qnury-es/astro preview",
    "lint": "pnpm --filter @qnury-es/astro lint; pnpm --filter @qnury-es/worker-api lint",
    "deploy": "pnpm --filter @qnury-es/astro run deploy",
    "deploy:api": "pnpm --filter @qnury-es/worker-api run deploy"
  },
  "keywords": [
    "astro",
    "cloudflare",
    "workers",
    "react",
    "typescript"
  ],
  "author": "Qnury",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  },
  "packageManager": "pnpm@10.8.0",
  "dependencies": {
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "sanitize-html": "^2.16.0",
    "zod": "^3.24.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.25.1",
    "@stylistic/eslint-plugin": "^4.2.0",
    "@types/node": "^22.15.3",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.3",
    "@typescript-eslint/parser": "^8.29.1",
    "eslint": "^9.25.1",
    "globals": "^16.0.0",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.31.1",
    "wrangler": "^4.10.0"
  }
}
