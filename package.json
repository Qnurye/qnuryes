{
  "name": "qnury-es",
  "private": true,
  "version": "0.0.0",
  "description": "Qnurye's personal website and API",
  "scripts": {
    "dev:astro": "pnpm --filter @qnury-es/astro dev",
    "dev:api": "pnpm --filter @qnury-es/worker-api dev",
    "dev:email": "pnpm --filter @qnury-es/worker-api dev:email",
    "build": "pnpm --filter @qnury-es/astro build",
    "preview": "pnpm --filter @qnury-es/astro preview",
    "lint": "pnpm --filter @qnury-es/astro lint; pnpm --filter @qnury-es/worker-api lint",
    "deploy": "pnpm --filter @qnury-es/astro run deploy",
    "deploy:api": "pnpm --filter @qnury-es/worker-api run deploy"
  },
  "keywords": [
    "astro",
    "cloudflare",
    "workers",
    "react",
    "typescript"
  ],
  "author": "Qnury",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  },
  "packageManager": "pnpm@10.15.0",
  "dependencies": {
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "sanitize-html": "^2.17.0",
    "zod": "^4.3.5"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@stylistic/eslint-plugin": "^5.7.0",
    "@types/node": "^25.0.9",
    "@types/react": "^19.2.8",
    "@types/react-dom": "^19.2.3",
    "@typescript-eslint/parser": "^8.53.1",
    "eslint": "^9.39.2",
    "globals": "^17.0.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.53.1",
    "wrangler": "^4.59.2"
  },
  "pnpm": {
    "overrides": {
      "undici": ">=7.18.2"
    }
  }
}
